generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  content   String?
  img       String?
  resume    String?
  published Boolean @default(false)
}

model User {
  id       Int    @id @default(autoincrement())
  name     String @unique()
  password String
}
